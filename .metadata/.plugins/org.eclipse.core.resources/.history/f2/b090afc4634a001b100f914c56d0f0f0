package ½ÃÇè;

public class Coding_6 {

	public static void main(String[] args) {
		String str="9B768C";
		String arry[]=new String[str.length()];
		for(int i=0;i<str.length();i++) {
			arry[i]=str.substring(i,i+1);
			char c=str.charAt(i);
			System.out.println(c);
		}
		for(String k:arry) {
			System.out.println(k);
			
		}
		
	}

	
	

}
