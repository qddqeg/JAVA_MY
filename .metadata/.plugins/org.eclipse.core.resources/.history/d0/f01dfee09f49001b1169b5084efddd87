package JAVA_1229;
import java.util.Scanner;
public class USER_Main {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		
		System.out.println("ID를 입력해 주세요.");
		String input=sc.nextLine();
		USER user=new USER();
		user.setID(input);
		System.out.println(user.getID());

	}

}
