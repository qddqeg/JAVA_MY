package JAVA_1214;
import java.util.Scanner;
public class part_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       Scanner sc=new Scanner(System.in);
       System.out.println("몇항을 계산하시겠습니까?");
       int pick=sc.nextInt();
       int array[]=new int[pick];
       System.out.println(array);
	}

}
