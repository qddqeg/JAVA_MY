package 시험;

import java.util.Scanner;

public class Coding_7 {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("숫자를 몇개를 받으시겠습니까? ");
		int pick= sc.nextInt();
		for(int i=1;i<=pick; i++) {
			System.out.println(i+" 번째 숫자입니다.");
			pick= sc.nextInt();
		}

	}

}
